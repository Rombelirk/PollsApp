{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "apollo:generate": "apollo codegen:generate --config=apollo.config.js --includes=screens/**/*.ts --target=typescript --addTypename --excludes=node_modules/*",
